{
    "ClusterName": "autotest-edw26-flex",
    "HDPVersion": "2.6",
    "ClusterType": "EDW-ETL: Apache Hive 1.2.1, Apache Spark 2.1",
    "Master": {
        "InstanceType": "m4.4xlarge",
        "VolumeType": "gp2",
        "VolumeSize": 32,
        "VolumeCount": 1,
        "Encrypted": false
    },
    "Worker": {
        "InstanceType": "m4.xlarge",
        "VolumeType": "gp2",
        "VolumeSize": 32,
        "VolumeCount": 1,
        "Encrypted": false,
        "InstanceCount": 3,
        "RecoveryMode": "AUTO"
    },
    "Compute": {
        "InstanceType": "m4.xlarge",
        "VolumeType": "gp2",
        "VolumeSize": 32,
        "VolumeCount": 1,
        "Encrypted": false,
        "InstanceCount": 0,
        "RecoveryMode": "AUTO"
    },
    "SSHKeyName": "seq-master",
    "RemoteAccess": "0.0.0.0/0",
    "WebAccess": true,
    "HiveJDBCAccess": true,
    "ClusterComponentAccess": true,
    "ClusterAndAmbariUser": "admin",
    "ClusterAndAmbariPassword": "admin",
    "Tags": {},
    "Autoscaling": {
        "Configurations": {
            "CooldownTime": 30,
            "ClusterMinSize": 3,
            "ClusterMaxSize": 100
        }
    },
    "InstanceRole": "CREATE",
    "FlexSubscription": {
        "Name": "default"
    }
}
