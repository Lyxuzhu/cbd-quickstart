{{/* $sshkeyname := "seq-master" */}}
  {
    "ClusterName": "{{$CLUSTER_NAME}}",
    "HDPVersion": "2.6",
    "ClusterType": "Enterprise Services: Apache Atlas, Apache Ranger",
    "Master": {
        "InstanceType": "m4.4xlarge",
        "VolumeType": "gp2",
        "VolumeSize": 32,
        "VolumeCount": 1,
        "Recipes": [
        ]
    },
    "Worker": {
        "InstanceType": "c3.2xlarge",
        "VolumeType": "ephemeral",
        "VolumeSize": 80,
        "VolumeCount": 2,
        "InstanceCount": 1,
        "RecoveryMode": "AUTO",
        "Recipes": [
        ]
    },
    "Compute": {
        "InstanceType": "c3.2xlarge",
        "VolumeType": "ephemeral",
        "VolumeSize": 80,
        "VolumeCount": 2,
        "InstanceCount": 1,
        "RecoveryMode": "MANUAL",
        "SpotPrice": "0.8",
        "Recipes": [
        ]
    },
    "SSHKeyName": "{{ $SSHKEY }}",
    "RemoteAccess": "0.0.0.0/0",
    "WebAccess": true,
    "HiveJDBCAccess": true,
    "ClusterComponentAccess": true,
    "ClusterAndAmbariUser": "admin",
    "ClusterAndAmbariPassword": "Admin123",
    "InstanceRole": "CREATE",
    "Network": {
      "VpcId": "{{$CLUSTER_VPCID}}",
      "SubnetId": "{{$CLUSTER_SUBNETID}}"
    },
    "HiveMetastore": {
      "Name": "{{$SHARED_HIVE_METASTORE}}"
    },
    "RangerMetastore": {
      "RangerAdminPassword": "admin",
      "Name": "admin",
      "URL": "{{$RDS_DB_ADDRESS}}:{{$RDS_DB_PORT}}/ranger",
      "Username": "{{$RDS_RANGER_USER}}",
      "Password": "{{$RDS_DB_PASSWORD}}",
      "DatabaseType": "POSTGRES" 
    },
    "CloudStoragePath": "{{$S3_STORAGE}}",
    "Ldap": "{{$SHARED_LDAP}}"
  }
